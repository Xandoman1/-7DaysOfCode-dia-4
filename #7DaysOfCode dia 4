

let chute = prompt("Digite um número de 0 a 10");
  
let computer = Math.floor(Math.random() * (10 - 0 + 1) + 1)

let contador = 1;

console.log(computer);

do {
  let chute = prompt("Digite um número de 0 a 10");
  {if (chute == computer)
    alert("ACERTOU")
  else
    alert("ERROU")
}
} while (++contador < 3);

console.log("O número correto era " + computer)
